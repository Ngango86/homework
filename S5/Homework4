
#include <stdio.h>
int main(){
	int n;
    printf("Nhap n=");
    scanf("%d" ,n);
    
	int i=1, count=0;

    if(n>0){
        while(i<=n){
            if(n%i==0){
                count++;
            }else{
                
            }
            i++;
        }
        if(count==2){
            printf("%d la so nguyen to",n);
        }else{
            printf("%d khong phai so nguyen to",n);
        }
    }else{
        print("ERROR!");
    }
        
}